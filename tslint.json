{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-multiline-js": false,
        "interface-name": false,
        "semicolon": false,
        "no-console": [false],
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template", "jsx-double"],
        "prettier": [
            true,
            {
                "singleQuote": true,
                "semi": false,
                "trailingComma": "all",
                "printWidth": 80
            }
        ]
    },
    "rulesDirectory": []
}